{
  "version": 2,
  "builds": [
    {
      "src": "excel-ai-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "excel_ai_backend/vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/excel_ai_backend/vercel_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/excel-ai-frontend/dist/$1"
    }
  ],
  "functions": {
    "excel_ai_backend/vercel_app.py": {
      "runtime": "python3.9"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  }
}
